    <define name="xptr-element">
        <element name="XPTR">
            <ref name="links-content"/>
            <empty/>
        </element>
    </define>

    <define name="links-content">
        <ref name="ID-attribute"/>
        <optional>
            <attribute name="DATE"/>
            <!--CDATA-->
        </optional>
        <optional>
            <attribute name="DOC"/>
            <!-- ENTITY -->
        </optional>
        <attribute name="FROM" a:defaultValue="ROOT">
            <value>ROOT</value>
        </attribute>
        <attribute name="TO" a:defaultValue="DITTO">
            <value>DITTO</value>
        </attribute>
        <ref name="resp-content"/>
        <optional>
            <attribute name="URL"/>
        </optional>
    </define>

    <define name="xref-element">
        <element name="XREF">
            <ref name="links-content"/>
            <zeroOrMore>
                <choice>
                    <text/>
                    <ref name="address-element"/>
                    <ref name="date-element"/>
                    <ref name="name-element"/>
                    <ref name="rs-element"/>
                    <ref name="time-element"/>
                    <ref name="corr-element"/>
                    <ref name="emph-element"/>
                    <ref name="foreign-element"/>
                    <ref name="soCalled-element"/>
                    <ref name="docTitle-element"/>
                    <ref name="xptr-element"/>
                    <ref name="xref-element"/>
                    <ref name="quote-element"/>
                </choice>
            </zeroOrMore>
        </element>
    </define>

    <!-- Inclusions start -->

    <define name="Div0-inclusion">
        <zeroOrMore>
            <choice>
                <ref name="researchNote-element"/>
                <ref name="title-element"/>
                <ref name="bibCit-element"/>
                <ref name="scholarNote-element"/>
                <ref name="keywordClass-element"/>
                <ref name="bibCits-element"/>
                <ref name="lb-element"/>
            </choice>
        </zeroOrMore>
    </define>

    <define name="Div1-inclusion">
        <zeroOrMore>
            <choice>
                <ref name="name-element"/>
                <ref name="place-element"/>
                <ref name="orgName-element"/>
                <ref name="rs-element"/>
                <ref name="sic-element"/>
                <ref name="corr-element"/>
                <ref name="emph-element"/>
                <ref name="quote-element"/>
                <ref name="foreign-element"/>
                <ref name="soCalled-element"/>
                <ref name="topic-element"/>
                <ref name="tGenre-element"/>
                <ref name="job-element"/>
                <ref name="xptr-element"/>
                <ref name="xref-element"/>
            </choice>
        </zeroOrMore>
    </define>

    <define name="narrow-inclusion">
        <!-- Inclusion from Div0 and Div1 except 
        elements in <date>*, keywordClass, name, 
        orgName, place, scholarNote, topic -->
        <zeroOrMore>
            <choice>
                <ref name="researchNote-element"/>
                <ref name="title-element"/>
                <ref name="bibCit-element"/>
                <ref name="bibCits-element"/>
                <ref name="lb-element"/>
                <ref name="rs-element"/>
                <ref name="sic-element"/>
                <ref name="corr-element"/>
                <ref name="emph-element"/>
                <ref name="quote-element"/>
                <ref name="foreign-element"/>
                <ref name="soCalled-element"/>
                <ref name="tGenre-element"/>
                <ref name="xptr-element"/>
                <ref name="xref-element"/>
            </choice>
        </zeroOrMore>
    </define>

    <define name="Div0narrow-inclusion">
        <!-- Inclusion from Div0 except 
            elements in <date>*, keywordClass, name, 
            orgName, place, scholarNote, topic -->
        <zeroOrMore>
            <choice>
                <ref name="researchNote-element"/>
                <ref name="title-element"/>
                <ref name="bibCit-element"/>
                <ref name="bibCits-element"/>
                <ref name="lb-element"/>
            </choice>
        </zeroOrMore>
    </define>

    <!-- common attributes -->

    <define name="ID-attribute">
        <optional>
            <attribute name="ID">
                <data type="ID"/>
            </attribute>
        </optional>
    </define>

    <define name="reg-attribute">
        <optional>
            <attribute name="REG" a:documentation="Regularized value"/>
        </optional>
    </define>

    <define name="sex-attribute">
        <attribute name="SEX">
            <choice>
                <value>FEMALE</value>
                <value>MALE</value>
                <value>UNKNOWN</value>
                <value>TRANSGENDERED</value>
            </choice>
        </attribute>
    </define>

