    <define name="workStatus-content">
        <attribute name="WORKSTATUS" a:defaultValue="SUB">
            <choice>
                <value type="token">SUB</value>
                <value type="token">RWT</value>
                <value type="token">CAS</value>
                <value type="token">RBV</value>
                <value type="token">CFT</value>
                <value type="token">CFB</value>
                <value type="token">CFC</value>
                <value type="token">REV</value>
                <value type="token">OLD</value>
                <value type="token">PUB</value>
                <value type="token">ENH</value>
            </choice>
        </attribute>
    </define>

    <define name="workValue-content">
        <attribute name="WORKVALUE" a:defaultValue="I">
            <choice>
                <value>I</value>
                <value>P</value>
                <value>C</value>
                <value>O</value>
            </choice>
        </attribute>
    </define>

    <define name="l-element">
        <element name="L">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="ENJAMB"/>
            </optional>
            <!--CDATA-->
            <optional>
                <attribute name="MET"/>
            </optional>
            <!--CDATA-->
            <optional>
                <attribute name="REAL"/>
            </optional>
            <!--CDATA-->
            <optional>
                <attribute name="RHYME"/>
            </optional>
            <!--CDATA-->
            <ref name="part-content"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <ref name="date-element"/>
                        <!--<ref name="name-element"/> included in Div1-inclusion-->
                        <!--<ref name="rs-element"/> included in Div1-inclusion-->
                        <ref name="time-element"/>
                        <!--<ref name="corr-element"/> included in Div1-inclusion-->
                        <!--<ref name="emph-element"/> included in Div1-inclusion-->
                        <!--<ref name="foreign-element"/> included in Div1-inclusion-->
                        <!--<ref name="soCalled-element"/> included in Div1-inclusion-->
                        <!--<ref name="quote-element"/> included in Div1-inclusion-->
                        <!--<ref name="topic-element"/> included in Div1-inclusion-->
                    </choice>
                </zeroOrMore>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="part-content">
        <attribute name="PART" a:defaultValue="N">
            <choice>
                <value>N</value>
                <value>Y</value>
                <value>I</value>
                <value>M</value>
                <value>F</value>
            </choice>
        </attribute>
    </define>

    <define name="lg-element">
        <element name="LG">
            <ref name="ID-attribute"/>
            <attribute name="TYPE"/>
            <!-- CDATA and check #CURRENT occurrence -->
            <attribute name="ORG" a:defaultValue="UNIFORM">
                <choice>
                    <value>UNIFORM</value>
                    <value>COMPOSITE</value>
                </choice>
            </attribute>
            <attribute name="SAMPLE" a:defaultValue="COMPLETE">
                <choice>
                    <value>COMPLETE</value>
                    <value>INITIAL</value>
                    <value>MEDIAL</value>
                    <value>FINAL</value>
                    <value>UNKNOWN</value>
                </choice>
            </attribute>
            <ref name="part-content"/>
            <optional>
                <attribute name="MET"/>
            </optional>
            <!--CDATA-->
            <optional>
                <attribute name="REAL"/>
            </optional>
            <!--CDATA-->
            <optional>
                <attribute name="RHYME"/>
            </optional>
            <!--CDATA-->
            <interleave>
                <group>
                    <ref name="head-element"/>
                    <oneOrMore>
                        <choice>
                            <ref name="l-element"/>
                            <ref name="lg-element"/>
                        </choice>
                    </oneOrMore>
                </group>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="head-element">
        <element name="HEAD">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="TYPE"/>
            </optional>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <ref name="date-element"/>
                        <!-- <ref name="name-element"/> included in Div1 inclusion -->
                        <!-- <ref name="rs-element"/> included in Div1 inclusion -->
                        <!-- <ref name="corr-element"/> included in Div1 inclusion -->
                        <!-- <ref name="emph-element"/> included in Div1 inclusion -->
                        <!-- <ref name="foreign-element"/> included in Div1 inclusion -->
                        <!-- <ref name="soCalled-element"/> included in Div1 inclusion -->
                        <!-- <ref name="quote-element"/> included in Div1 inclusion -->
                        <!-- <ref name="topic-element"/> included in Div1 inclusion -->
                    </choice>
                </zeroOrMore>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="p-element">
        <element name="P">
            <ref name="ID-attribute"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <ref name="date-element"/>
                        <ref name="dateStruct-element"/>
                        <ref name="dateRange-element"/>
                        <ref name="time-element"/>
                        <!--<ref name="name-element"/>-->
                        <!--<ref name="orgName-element"/>-->
                        <!--<ref name="place-element"/>-->
                        <!--<ref name="emph-element"/>-->
                        <!--<ref name="foreign-element"/>-->
                        <!--<ref name="soCalled-element"/>-->
                        <!--<ref name="quote-element"/>-->
                        <!--<ref name="corr-element"/>-->
                        <!--<ref name="rs-element"/>-->
                        <!--<ref name="title-element"/>-->
                        <ref name="lg-element"/>
                        <ref name="l-element"/>
                        <!--<ref name="topic-element"/>-->
                    </choice>
                </zeroOrMore>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="dateStruct-element">
        <!-- dateStruct not used anymore by authors, can be removed from 
            schemas after a batch change.-->
        <element name="DATESTRUCT">
            <ref name="date-attributes"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <ref name="year-element"/>
                        <ref name="ocassion-element"/>
                        <ref name="season-element"/>
                        <ref name="month-element"/>
                        <ref name="time-element"/>
                        <ref name="week-element"/>
                        <ref name="day-element"/>
                    </choice>
                </zeroOrMore>
                <ref name="narrow-inclusion"/>
                <!-- narrow inclusion: 
                   from Div0 and Div1 except elements in DTD exclusion: 
                   dateStruct , dateRange , keywordClass , name , orgName , 
                   place , date , scholarNote , topic? -->
            </interleave>
        </element>
    </define>

    <define name="datestruct-content">
        <ref name="ID-attribute"/>
        <optional>
            <attribute name="VALUE"/>
        </optional>
        <!-- CDATA in DTD -->
        <optional>
            <attribute name="TYPE"/>
        </optional>
        <ref name="reg-attribute"/>
        <ref name="certainty-content"/>
    </define>

    <define name="year-element">
        <element name="YEAR">
            <ref name="datestruct-content"/>
            <interleave>
                <ref name="narrow-inclusion"/>
                <text/>
            </interleave>
        </element>
    </define>

    <define name="ocassion-element">
        <element name="OCCASION">
            <ref name="datestruct-content"/>
            <interleave>
                <ref name="narrow-inclusion"/>
                <text/>
            </interleave>
        </element>
    </define>

    <define name="season-element">
        <element name="SEASON">
            <ref name="datestruct-content"/>
            <interleave>
                <ref name="narrow-inclusion"/>
                <text/>
            </interleave>
        </element>
    </define>

    <define name="month-element">
        <element name="MONTH">
            <ref name="datestruct-content"/>
            <interleave>
                <ref name="narrow-inclusion"/>
                <text/>
            </interleave>
        </element>
    </define>

    <define name="week-element">
        <element name="WEEK">
            <ref name="datestruct-content"/>
            <interleave>
                <ref name="narrow-inclusion"/>
                <text/>
            </interleave>
        </element>
    </define>

    <define name="day-element">
        <element name="DAY">
            <ref name="datestruct-content"/>
            <interleave>
                <ref name="narrow-inclusion"/>
                <text/>
            </interleave>
        </element>
    </define>

    <define name="dateRange-element">
        <!-- exclusion of dateStruct , 
        dateRange , keywordClass , name , orgName , place , date , 
        scholarNote , topic? -->
        <element name="DATERANGE">
            <ref name="ID-attribute"/>
            <ref name="calendar-content"/>
            <ref name="certainty-content"/>
            <optional>
                <attribute name="FROM"/>
            </optional>
            <optional>
                <attribute name="TO"/>
            </optional>
            <optional>
                <attribute name="EXACT">
                    <choice>
                        <value>TO</value>
                        <value>FROM</value>
                        <value>BOTH</value>
                        <value>NEITHER</value>
                    </choice>
                </attribute>
            </optional>
            <interleave>
                <text/>
                <ref name="narrow-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="orgName-element">
        <element name="ORGNAME">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="STANDARD"/>
            </optional>
            <optional>
                <attribute name="ORGTYPE"/>
            </optional>
            <ref name="reg-attribute"/>
            <mixed>
                <!-- changed interleave to mixed, and deleted <text> -->
                <ref name="narrow-inclusion"/>
            </mixed>
        </element>
    </define>

    <define name="place-element">
        <!-- has exclusion -->
        <element name="PLACE">
            <ref name="ID-attribute"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <element name="PLACENAME">
                            <ref name="placeChild-content"/>
                        </element>
                        <ref name="address-element"/>
                        <element name="SETTLEMENT">
                            <ref name="placeChild-content"/>
                        </element>
                        <element name="REGION">
                            <ref name="placeChild-content"/>
                        </element>
                        <element name="GEOG">
                            <ref name="placeChild-content"/>
                        </element>
                        <element name="AREA">
                            <ref name="placeChild-content"/>
                        </element>
                        <ref name="narrow-inclusion"/>
                    </choice>
                </zeroOrMore>
            </interleave>
        </element>
    </define>

    <define name="placeChild-content">
        <ref name="ID-attribute"/>
        <optional>
            <attribute name="CURRENT"/>
        </optional>
        <ref name="reg-attribute"/>
        <interleave>
            <text/>
            <ref name="narrow-inclusion"/>
        </interleave>
    </define>

    <define name="title-element">
        <element name="TITLE">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="TITLETYPE">
                    <choice>
                        <value>MONOGRAPHIC</value>
                        <value>ANALYTIC</value>
                        <value>JOURNAL</value>
                        <value>SERIES</value>
                        <value>UNPUBLISHED</value>
                    </choice>
                </attribute>
            </optional>
            <ref name="reg-attribute"/>
            <optional>
                <attribute name="REND"/>
            </optional>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <!--<ref name="title-element"/>-->
                        <ref name="date-element"/>
                        <!--<ref name="name-element"/>-->
                        <!--<ref name="emph-element"/>-->
                        <!--<ref name="foreign-element"/>-->
                        <!--<ref name="soCalled-element"/>-->
                        <!--<ref name="quote-element"/>-->
                        <!--<ref name="topic-element"/>-->
                    </choice>
                </zeroOrMore>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="topic-element">
        <element name="TOPIC">
            <!-- exclusion of dateStruct , dateRange , 
            keywordClass , name , topic , orgName , place , scholarNote -->
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="STANDARD"/>
            </optional>
            <!-- CDATA -->
            <interleave>
                <text/>
                <ref name="narrow-inclusion"/>
            </interleave>
        </element>
    </define>

    <!-- Div0 starts -->

    <define name="Div0-element">
        <!-- this Div0 is used in events only -->
        <element name="DIV0">
            <ref name="ID-attribute"/>
            <interleave>
                <group>
                    <optional>
                        <ref name="heading-element"/>
                    </optional>
                    <zeroOrMore>
                        <ref name="Div1-element"/>
                    </zeroOrMore>
                </group>
                <ref name="Div0-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="Div1-element">
        <element name="DIV1">
            <ref name="ID-attribute"/>
            <interleave>
                <oneOrMore>
                    <group>
                        <choice>
                            <optional>
                                <ref name="heading-element"/>
                            </optional>
                            <zeroOrMore>
                                <ref name="chronEvent-element"/>
                            </zeroOrMore>
                            <zeroOrMore>
                                <ref name="worksCited-element"/>
                            </zeroOrMore>
                        </choice>
                    </group>
                </oneOrMore>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="heading-element">
        <!-- exclusion: datestruct, daterange, date,
        keywordclass, name, orgname, place, topic , bibcit (bio DTD)-->
        <element name="HEADING">
