<?xml version="1.0" encoding="UTF-8"?>
<!-- common schema   *in progress*
    for use by Biography, Events and Writing schemas
    written 2010-03-31 by Mariana Paredes-Olea,
    based on events.dtd and biograph.dtd (SGML)
    Copyright 2010 The Orlando Project -->

<!-- TO DO:-->
<!--Check SGML minimization rules that allow for non-closed tags:
    xptr , xref-->
<!-- Redefine default values - schematron -->
<!-- Check inclusions and exclusions -->
<!-- check all mixed patterns against dtds (text) -->

<grammar xmlns="http://relaxng.org/ns/structure/1.0"
    xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
    datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">

    <start>
        <ref name="Common"/>
    </start>

    <define name="Common">
        <!-- check -->
        <ref name="OrlandoHeader-element"/>
    </define>

    <!-- OrlandoHeader starts -->

    <define name="OrlandoHeader-element">
        <element name="ORLANDOHEADER">
            <ref name="ID-attribute"/>
            <optional> <!-- check in dtd -->
                <attribute name="TYPE" a:defaultValue="TEXT"/>
            </optional>
            <ref name="fileDesc-element"/>
            <ref name="revisionDesc-element"/>
        </element>
    </define>

    <define name="fileDesc-element">
        <element name="FILEDESC">
            <ref name="ID-attribute"/>
            <ref name="titleStmt-element"/>
            <optional>
                <ref name="docExtent-element"/>
            </optional>
            <ref name="publicationStmt-element"/>
            <ref name="sourceDesc-element"/>
        </element>
    </define>

    <define name="titleStmt-element">
        <element name="TITLESTMT">
            <ref name="ID-attribute"/>
            <group>
                <oneOrMore>
                    <ref name="docTitle-element"/>
                </oneOrMore>
                <zeroOrMore>
                    <choice>
                        <element name="DOCAUTHOR">
                            <ref name="ID-attribute"/>
                            <text/>
                        </element>
                        <element name="DOCEDITOR">
                            <ref name="ID-attribute"/>
                            <attribute name="ROLE" a:defaultValue="EDITOR"> </attribute>
                            <text/>
                        </element>
                    </choice>
                </zeroOrMore>
            </group>
        </element>
    </define>

    <define name="docTitle-element">
        <element name="DOCTITLE">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="TYPE"/>
            </optional>
            <zeroOrMore>
                <choice>
                    <text/>
                    <ref name="date-element"/>
                    <ref name="emph-element"/>
                    <ref name="foreign-element"/>
                    <ref name="soCalled-element"/>
                    <ref name="quote-element"/>
                    <ref name="rs-element"/>
                </choice>
            </zeroOrMore>
        </element>
    </define>

    <define name="date-element">
        <element name="DATE">
            <ref name="date-attributes"/>
            <interleave>
                <text/>
                <ref name="narrow-inclusion"/>
                <!-- Inclusion from Div1 and Div0 except elements
                    in DTD exclusion (dateStruct, dateRange, keywordClass,
                    name, orgName, place, scholarNote, topic) Bio also excludes-->
            </interleave>
        </element>
    </define>

    <define name="date-attributes">
        <ref name="ID-attribute"/>
        <ref name="calendar-content"/>
        <optional>
            <attribute name="VALUE"/>
        </optional>
        <ref name="certainty-content"/>
    </define>

    <define name="calendar-content">
        <optional>
            <attribute name="CALENDAR" a:defaultValue="NEWSTYLE">
                <choice>
                    <value>NEWSTYLE</value>
                    <value>BC</value>
                </choice>
            </attribute>
        </optional>
    </define>

    <define name="certainty-content">
        <!-- check if certainty , calendar are optional attributes -->
        <optional>
            <attribute name="CERTAINTY" a:defaultValue="CERT">
                <choice>
                    <value>CERT</value>
                    <value>C</value>
                    <value>BY</value>
                    <value>AFTER</value>
                    <value>UNKNOWN</value>
                    <value>ROUGHLYDATED</value>
                </choice>
            </attribute>
        </optional>
    </define>

    <define name="emph-element">
        <element name="EMPH">
            <ref name="ID-attribute"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <ref name="date-element"/>
                        <!--<ref name="name-element"/>-->
                        <!--<ref name="rs-element"/>-->
                        <ref name="time-element"/>
                        <!--<ref name="corr-element"/>-->
                        <!--<ref name="emph-element"/>-->
                        <!--<ref name="foreign-element"/>-->
                        <!--<ref name="soCalled-element"/>-->
                        <!--<ref name="quote-element"/>-->
                        <!--<ref name="topic-element"/>-->
                    </choice>
                </zeroOrMore>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="foreign-element">
        <element name="FOREIGN">
            <ref name="ID-attribute"/>
            <ref name="reg-attribute"/>
            <optional>
                <attribute name="LANG"/>
            </optional>
            <!-- change to xml:lang ?-->
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <ref name="date-element"/>
                        <!--<ref name="name-element"/>-->
                        <!--<ref name="rs-element"/>-->
                        <ref name="time-element"/>
                        <!--<ref name="corr-element"/>-->
                        <!--<ref name="emph-element"/>-->
                        <!--<ref name="foreign-element"/>-->
                        <!--<ref name="soCalled-element"/>-->
                        <!--<ref name="quote-element"/>-->
                        <!--<ref name="topic-element"/>-->
                    </choice>
                </zeroOrMore>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="soCalled-element">
        <element name="SOCALLED">
            <ref name="ID-attribute"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <ref name="date-element"/>
                        <!--<ref name="name-element"/>
                        <ref name="rs-element"/>-->
                        <ref name="time-element"/>
                        <!--<ref name="corr-element"/>
                        <ref name="emph-element"/>
                        <ref name="foreign-element"/>
                        <ref name="soCalled-element"/>-->
                        <!--<ref name="topic-element"/>-->
                    </choice>
                </zeroOrMore>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="quote-element">
        <element name="QUOTE">
            <ref name="ID-attribute"/>
            <attribute name="DIRECT" a:defaultValue="UNSPECIFIED">
                <choice>
                    <value>UNSPECIFIED</value>
                    <value>Y</value>
                    <value>N</value>
                </choice>
            </attribute>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <ref name="corrquoteitem-group"/>
                        <text></text>
                    </choice>
                </zeroOrMore>
            </interleave>
        </element>
    </define>

    <define name="rs-element">
        <element name="RS">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="KEY"/>
            </optional>
            <ref name="reg-attribute"/>
            <optional>
                <attribute name="TYPE"/>
            </optional>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <ref name="date-element"/>
                        <!--<ref name="name-element"/>-->
                        <!--<ref name="rs-element"/>-->
                        <ref name="time-element"/>
                        <!--<ref name="corr-element"/>-->
                        <!--<ref name="emph-element"/>-->
                        <!--<ref name="foreign-element"/>-->
                        <!--<ref name="soCalled-element"/>-->
                        <!--<ref name="topic-element"/>-->
                    </choice>
                </zeroOrMore>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="docExtent-element">
        <element name="DOCEXTENT">
            <ref name="ID-attribute"/>
            <zeroOrMore>
                <choice>
                    <text/>
                    <ref name="date-element"/>
                </choice>
            </zeroOrMore>
        </element>
    </define>

    <define name="publicationStmt-element">
        <element name="PUBLICATIONSTMT">
            <ref name="ID-attribute"/>
            <choice>
                <oneOrMore>
                    <ref name="p-element"/>
                </oneOrMore>
                <oneOrMore>
                    <interleave>
                        <ref name="authority-element"/>
                        <ref name="publicationStmt-group"/>
                    </interleave>
                </oneOrMore>
            </choice>
        </element>
    </define>

    <define name="authority-element">
        <element name="AUTHORITY">
            <ref name="ID-attribute"/>
            <zeroOrMore>
                <choice>
                    <text/>
                    <ref name="address-element"/>
                    <ref name="date-element"/>
                    <ref name="name-element"/>
                    <ref name="emph-element"/>
                    <ref name="foreign-element"/>
                    <ref name="soCalled-element"/>
                    <ref name="topic-element"/>
                </choice>
            </zeroOrMore>
        </element>
    </define>

    <define name="publicationStmt-group">
        <oneOrMore>
            <group>
                <optional>
                    <ref name="pubPlace-element"/>
                </optional>
                <optional>
                    <ref name="address-element"/>
                </optional>
                <zeroOrMore>
                    <ref name="idno-element"/>
                </zeroOrMore>
                <optional>
                    <ref name="availability-element"/>
                </optional>
                <zeroOrMore>
                    <ref name="projectDesc-element"/>
                </zeroOrMore>
                <zeroOrMore>
                    <ref name="editorialDecl-element"/>
                </zeroOrMore>
                <optional>
                    <ref name="date-element"/>
                </optional>
            </group>
        </oneOrMore>
    </define>

    <define name="pubPlace-element">
        <element name="PUBPLACE">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="KEY"/>
            </optional>
            <ref name="reg-attribute"/>
            <zeroOrMore>
                <choice>
                    <text/>
                    <ref name="address-element"/>
                    <ref name="date-element"/>
                    <ref name="name-element"/>
                    <ref name="rs-element"/>
                    <ref name="time-element"/>
                    <ref name="corr-element"/>
                    <ref name="emph-element"/>
                    <ref name="foreign-element"/>
                    <ref name="soCalled-element"/>
                    <ref name="docTitle-element"/>
                    <ref name="topic-element"/>
                </choice>
            </zeroOrMore>
        </element>
    </define>

    <define name="address-element">
        <element name="ADDRESS">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="CURRENT" a:documentation="Current name for geographical location"/>
            </optional>
            <ref name="reg-attribute"/>
            <interleave>
                <oneOrMore>
                    <ref name="addrLine-element"/>
                </oneOrMore>
                <ref name="narrow-inclusion"/>
                <!-- inclusion for address -->
            </interleave>
        </element>
    </define>

    <define name="addrLine-element">
        <element name="ADDRLINE">
            <ref name="ID-attribute"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
                        <ref name="date-element"/>
                        <ref name="name-element"/>
                        <!-- <ref name="rs-element"/> included in addrLine's narrow-inclusion -->
                        <ref name="time-element"/>
                        <!-- <ref name="corr-element"/> included in addrLine's narrow-inclusion -->
                        <!-- <ref name="emph-element"/> included in addrLine's narrow-inclusion -->
                        <!-- <ref name="foreign-element"/> included in addrLine's narrow-inclusion -->
                        <!-- <ref name="soCalled-element"/> included in addrLine's narrow-inclusion -->
                        <ref name="topic-element"/>
                    </choice>
                </zeroOrMore>
                <ref name="narrow-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="name-element">
        <!-- Exclusion: dateStruct, dateRange, 
        keywordClass, name, orgName, place, scholarNote, topic -->
        <element name="NAME">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="STANDARD"/>
            </optional>
            <interleave>
                <text/>
                <ref name="narrow-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="time-element">
        <element name="TIME">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="VALUE"/>
            </optional>
            <optional>
                <attribute name="TYPE"/>
            </optional>
            <optional>
                <attribute name="ZONE"/>
            </optional>
            <ref name="certainty-content"/>
            <interleave>
                <text/>
                <ref name="narrow-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="corr-element">
        <element name="CORR">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="SIC"/>
            </optional>
            <!-- CDATA -->
            <optional>
                <attribute name="CERT"/>
            </optional>
            <!-- CDATA -->
            <ref name="resp-content"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <ref name="corrquoteitem-group"/>
                    </choice>
                </zeroOrMore>
            </interleave>
        </element>
    </define>

    <define name="corrquoteitem-group">
        <!-- content used in elements corr, quote, and item -->
        <interleave>
            <group>
                <!-- grouped so interleave does not interfere with choices of either
                l/lg/p followed by 0+ l,lg,p,quote OR 0+ date,name,rs,etc. - test and revise-->
                <choice>
                    <group>
                        <choice>
                            <ref name="l-element"/>
                            <ref name="lg-element"/>
                            <ref name="p-element"/>
                        </choice>
                        <zeroOrMore>
                            <choice>
                                <ref name="l-element"/>
                                <ref name="lg-element"/>
                                <ref name="p-element"/>
                                <ref name="quote-element"/>
                            </choice>
                        </zeroOrMore>
                    </group>
                    <zeroOrMore>
                        <choice>
                            <!--<text/>-->
                            <ref name="date-element"/>
                            <ref name="name-element"/>
                            <ref name="rs-element"/>
                            <ref name="time-element"/>
                            <ref name="corr-element"/>
                            <ref name="emph-element"/>
                            <ref name="foreign-element"/>
                            <ref name="soCalled-element"/>
                            <ref name="quote-element"/>
                            <ref name="topic-element"/>
                            <!-- Div1 inclusion has many overlapping elements, 
                                commenting overlaps would have altered corr and quote's 
                                group choices; non-overlapping elements below-->
                            <ref name="place-element"/>
                            <ref name="orgName-element"/>
                            <ref name="sic-element"/>
                            <ref name="tGenre-element"/>
                            <ref name="xptr-element"/>
                            <ref name="xref-element"/>
                        </choice>
                    </zeroOrMore>
                </choice>
            </group>
            <ref name="Div0-inclusion"/>
        </interleave>
    </define>

    <define name="idno-element">
        <element name="IDNO">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="TYPE"/>
            </optional>
            <text/>
        </element>
    </define>

    <define name="availability-element">
        <element name="AVAILABILITY">
            <ref name="ID-attribute"/>
            <optional>
                <attribute name="STATUS">
                    <optional>
                        <choice>
                            <value>FREE</value>
                            <value>UNKNOWN</value>
                            <value>RESTRICTED</value>
                        </choice>
                    </optional>
                </attribute>
            </optional>
            <oneOrMore>
                <ref name="p-element"/>
            </oneOrMore>
        </element>
    </define>

    <define name="projectDesc-element">
        <element name="PROJECTDESC">
            <ref name="ID-attribute"/>
            <oneOrMore>
                <ref name="p-element"/>
            </oneOrMore>
        </element>
    </define>

    <define name="editorialDecl-element">
        <element name="EDITORIALDECL">
            <ref name="ID-attribute"/>
            <oneOrMore>
                <ref name="p-element"/>
            </oneOrMore>
        </element>
    </define>

    <define name="sourceDesc-element">
        <element name="SOURCEDESC">
            <ref name="ID-attribute"/>
            <text/>
        </element>
    </define>

    <define name="revisionDesc-element">
        <element name="REVISIONDESC">
            <ref name="ID-attribute"/>
            <oneOrMore>
                <ref name="responsibility-element"/>
            </oneOrMore>
        </element>
    </define>

    <define name="responsibility-element">
        <element name="RESPONSIBILITY">
            <ref name="ID-attribute"/>
            <ref name="resp-content"/>
            <ref name="workStatus-content"/>
            <ref name="workValue-content"/>
            <interleave>
                <group>
                    <ref name="date-element"/>
                    <zeroOrMore>
                        <ref name="item-element"/>
                    </zeroOrMore>
                </group>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="item-element">
        <element name="ITEM">
            <ref name="ID-attribute"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <ref name="corrquoteitem-group"/>
                        <text></text>
                    </choice>
                </zeroOrMore>
            </interleave>
        </element>
    </define>

