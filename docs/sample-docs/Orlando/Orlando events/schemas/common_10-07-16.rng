                    <oneOrMore>
                        <choice>
                            <ref name="dataStruct-element"/>
                            <ref name="shortProse-element"/>
                            <zeroOrMore>
                                <ref name="responsibility-element"/>
                            </zeroOrMore>
                        </choice>
                    </oneOrMore>
                </group>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="chronStruct-element">
        <element name="CHRONSTRUCT">
            <ref name="ID-attribute"/>
            <ref name="chronColumnRelevance-content"/>
            <interleave>
                <group>
                    <choice>
                        <ref name="dateRange-element"/>
                        <ref name="dateStruct-element"/>
                        <ref name="date-element"/>
                    </choice>
                    <ref name="chronProse-element"/>
                </group>
                <ref name="Div0-inclusion"/>
                <ref name="Div1-inclusion"/>
            </interleave>
        </element>
    </define>

    <define name="chronColumnRelevance-content">
        <attribute name="CHRONCOLUMN">
            <choice>
                <value>NATIONALINTERNATIONAL</value>
                <value>BRITISHWOMENWRITERS</value>
                <value>WRITINGCLIMATE</value>
                <value>SOCIALCLIMATE</value>
            </choice>
        </attribute>
        <attribute name="RELEVANCE">
            <choice>
                <value>SELECTIVE</value>
                <value>PERIOD</value>
                <value>DECADE</value>
                <value>COMPREHENSIVE</value>
            </choice>
        </attribute>
        <ref name="resp-content"/>
        <optional>
            <attribute name="CHRONCOLUMN1">
                <choice>
                    <value>NATIONALINTERNATIONAL1</value>
                    <value>BRITISHWOMENWRITERS1</value>
                    <value>WRITINGCLIMATE1</value>
                    <value>SOCIALCLIMATE1</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="CHRONCOLUMN2">
                <choice>
                    <value>NATIONALINTERNATIONAL2</value>
                    <value>BRITISHWOMENWRITERS2</value>
                    <value>WRITINGCLIMATE2</value>
                    <value>SOCIALCLIMATE2</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="CHRONCOLUMN3">
                <choice>
                    <value>NATIONALINTERNATIONAL3</value>
                    <value>BRITISHWOMENWRITERS3</value>
                    <value>WRITINGCLIMATE3</value>
                    <value>SOCIALCLIMATE3</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="RELEVANCE1">
                <choice>
                    <value>SELECTIVE1</value>
                    <value>PERIOD1</value>
                    <value>DECADE1</value>
                    <value>COMPREHENSIVE1</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="RELEVANCE2">
                <choice>
                    <value>SELECTIVE2</value>
                    <value>PERIOD2</value>
                    <value>DECADE2</value>
                    <value>COMPREHENSIVE2</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="RELEVANCE3">
                <choice>
                    <value>SELECTIVE3</value>
                    <value>PERIOD3</value>
                    <value>DECADE3</value>
                    <value>COMPREHENSIVE3</value>
                </choice>
            </attribute>
        </optional>
    </define>

    <define name="chronProse-element">
        <element name="CHRONPROSE">
            <ref name="ID-attribute"/>
            <interleave>
                <zeroOrMore>
                    <choice>
                        <text/>
