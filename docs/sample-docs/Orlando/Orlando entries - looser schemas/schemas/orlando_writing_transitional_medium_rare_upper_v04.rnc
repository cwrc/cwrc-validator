start = element WRITING { attlist.Writing, Div0 }
# Div0: +( researchNote , title , bibCit , scholarNote , keywordClass , bibCits , lb)
Div0 =
    element DIV0 {
        attlist.Div0,
        included_heading?,
        (included_standard | included_nonStandardDescriptor),
        included_authorSummary?,
        included_Div1*,
        included_worksCited?,
        included_biogProse?
    }
# Div1: +(dateStruct , date , dateRange , time , name , place , orgName , emph , foreign , soCalled , quote , sic , corr , rs , l , lg , topic , xref , xptr)
Div1 = element DIV1 { attlist.Div1, included_heading?, (included_textScope*, included_Div2+)* }
Div2 =
    element DIV2 { attlist.Div2, heading?, textScope*, (production | reception | textualFeatures)* }
Div3 = element DIV3 { attlist.Div3, heading?, (chronStruct | dataStruct | shortProse)+ }
ExtentOfOeuvre = element EXTENTOFOEUVRE { attlist.ExtentOfOeuvre, text }
addrLine =
    element ADDRLINE {
        attlist.addrLine,
        (text | date | name | rs | time | corr | emph | foreign | soCalled | topic)*
    }
address = element ADDRESS { attlist.address, addrLine+ }
area = element AREA { attlist.area, text }
# authorSummary: +(ExtentOfOeuvre , tGenre , genericRange , dateStruct , date , dateRange , time , name , place , orgName , emph , foreign , quote , soCalled , sic , corr , rs , lg , l , topic)
authorSummary = element AUTHORSUMMARY { attlist.authorSummary, included_shortProse }
authority =
    element AUTHORITY {
        attlist.authority, (text | address | date | name | emph | foreign | soCalled)*
    }
availability = element AVAILABILITY { attlist.availability, p+ }
bibCit = element BIBCIT { attlist.bibCit, text }
bibCits = element BIBCITS { attlist.bibCits, bibCit+ }
biogProse = element BIOGPROSE { attlist.biogProse, text }
chronProse =
    element CHRONPROSE {
        attlist.chronProse,
        (text | rs | time | corr | emph | foreign | soCalled | title | quote | l | lg)*
    }
chronStruct =
    element CHRONSTRUCT { attlist.chronStruct, (dateRange | dateStruct | date), chronProse }
# element CORR (((l | lg | p) , (l | lg | p | quote)*) | (#PCDATA | date | name | rs | time | corr | emph | foreign | soCalled | quote | topic)*)
corr =
    element CORR {
        attlist.corr,
        (text
         | date
         | name
         | rs
         | time
         | corr
         | emph
         | foreign
         | soCalled
         | quote
         | topic
         | l
         | lg
         | p)*
    }
