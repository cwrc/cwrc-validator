start = element BIOGRAPHY { attlist.Biography, Div0 }
# Div0: +(bibCit , title , keywordClass , researchNote , scholarNote , bibCits , lb)
Div0 =
    element DIV0 {
        attlist.Div0,
        ( included_researchNote*
        | included_heading?
        | included_standard?
        | included_Div1+
        | included_worksCited?
        | included_biogProse? ) +
    }
Div1 =
    element DIV1 {
        attlist.Div1,
        included_heading?,
        (included_personName
         | included_birth
         | included_death
         | included_culturalFormation
         | included_education*
         | included_family
         | included_friendsAssociates
         | included_health*
         | included_intimateRelationships*
         | included_location*
         | included_occupation
         | included_politics
         | included_leisureAndSociety
         | included_violence
         | included_wealth
         | included_otherLifeEvent)
    }
Div2 = element DIV2 { attlist.Div2, heading?, (chronStruct | dataStruct | shortProse | Div3*)+ }
Div3 =
    element DIV3 {
        attlist.Div3, heading?, (chronStruct | dataStruct | shortProse | extendedProse)+
    }
addrLine =
    element ADDRLINE {
        attlist.addrLine,
        (text | date | name | rs | time | corr | emph | foreign | soCalled | topic)*
    }
address = element ADDRESS { attlist.address, addrLine+ }
area = element AREA { attlist.area, text }
authority =
    element AUTHORITY {
        attlist.authority, (text | address | date | name | emph | foreign | soCalled | topic)*
    }
availability = element AVAILABILITY { attlist.availability, p+ }
award = element AWARD { attlist.award, text }
bibCit = element BIBCIT { attlist.bibCit, text }
bibCits = element BIBCITS { attlist.bibCits, bibCit+ }
attlist.bibCits &= empty
biogProse = element BIOGPROSE { attlist.biogProse, text }
# birth: +(birthPosition)
birth = element BIRTH { attlist.birth, (included_chronStruct | included_Div2)* }
birthPosition = element BIRTHPOSITION { attlist.birthPosition, text }
birthname =
    element BIRTHNAME {
        attlist.birthname,
        ((surname*, given*) | (given*, surname*))
    }
cause = element CAUSE { attlist.cause, text }
childlessness = element CHILDLESSNESS { attlist.childlessness, text }
children = element CHILDREN { attlist.children, text }
