class = element CLASS { attlist.class, text }
classIssue = element CLASSISSUE { attlist.classIssue, (chronStruct* | Div2*)+ }
companion = element COMPANION { attlist.companion, text }
contestedBehaviour = element CONTESTEDBEHAVIOUR { attlist.contestedBehaviour, text }
# element CORR (((l | lg | p) , (l | lg | p | quote)*) | (#PCDATA | date | name | rs | time | corr | emph | foreign | soCalled | quote | topic)*)
corr =
    element CORR {
        attlist.corr,
        (text
         | date
         | name
         | rs
         | time
         | corr
         | emph
         | foreign
         | soCalled
         | quote
         | topic
         | l
         | lg
         | p)*
    }
# culturalFormation: +(raceColour , class , nationalHeritage , nationality , geogHeritage , ethnicity , denomination , language , politicalAffiliation , sexualIdentity)
culturalFormation =
    element CULTURALFORMATION {
        attlist.culturalFormation,
        (included_chronStruct
         | included_Div2
         | included_classIssue
         | included_nationalityIssue
         | included_raceAndEthnicity
         | included_religion
         | included_sexuality)*
    }
